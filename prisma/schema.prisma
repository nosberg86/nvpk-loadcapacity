generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model sheet {
  id     Int  @id @default(autoincrement())
  Type      String
  length    Float
  width     Float
  thickness Float
  weight    Float
  stock     Int
  createdAt DateTime @default(now())
}

model Truck {
  id          Int  @id @default(autoincrement())
  model       String
  length      Float
  width       Float
  height      Float
  capacity    Float
  createdAt   DateTime @default(now())
}